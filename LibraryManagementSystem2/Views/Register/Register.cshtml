    <link href="~/webregister/css/style.css" type="text/css" rel="stylesheet" media="all">
    <!-- //css files -->

    <link rel="stylesheet" href="~/webregister/css/font-awesome.css"> <!-- Font-Awesome-Icons-CSS -->

</head>
<!-- body starts -->
<body>
    <!-- section -->
    <section class="register">
        <div class="register-full">
            <div class="register-left">
                <div class="register">
                    <div class="logo">
                        <a href="#"><span class="fa fa-graduation-cap" aria-hidden="true"></span></a>
                    </div>
                    <h1>LIBRARY MANAGEMENT SYSTEM WITH MVC</h1>
                    <p>
                        Welcome to our library! We're delighted to have you here. To become a member and enjoy all of our services,
                        please complete the registration form located on the right side of the panel by providing all the required information accurately."
                    </p>
                    <div class="content3">
                        <ul>
                            <li><a class="" href="/Login/Login/"> Log in</a></li>
                            <li><a class="read" href="/Showcase/Index/"> Back to website</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="register-right">
                <div class="register-in">
                    <h2><span class="fa fa-pencil"></span> Registration Panel</h2>
                    <div class="register-form">
                        <form action="#" method="post">
                            <div class="fields-grid">
                                <div class="styled-input agile-styled-input-top">
                                    <input type="text" name="NAME" required="">
                                    <label>Name</label>
                                    <span></span>
                                </div>
                                <div class="styled-input agile-styled-input-top">
                                    <input type="text" name="SURNAME" required="">
                                    <label>Surname</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="email" name="EMAIL" required="">
                                    <label>Email</label>
                                    <span></span>
                                </div>
                                <div class="styled-input agile-styled-input-top">
                                    <input type="text" name="USERNAME" required="">
                                    <label>Username</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="tel" name="PHONE" required="">
                                    <label>Phone Number</label>
                                    <span></span>
                                </div>
                                <div class="styled-input">
                                    <input type="password" name="PASSWORD" required="">
                                    <label>Password</label>
                                    <span></span>
                                </div>
                                <div class="styled-input agile-styled-input-top">
                                    <select id="category2" required="">
                                        <option value="">Choose your school*</option>
                                        <option value="">Bogazici University </option>
                                        <option value="">Beykoz University </option>
                                        <option value="">Marmara University</option>
                                        <option value="">Istanbul University</option>
                                        <option value="">Istanbul Bilgi University</option>
                                        <option value="">Istanbul Aydın University</option>
                                        <option value="">Istinye University</option>
                                        <option value="">Yeditepe University</option>
                                        <option value="">Yıldız Technical University</option>
                                        <option value="">Ozyegin University</option>
                                        
                                    </select>
                                    <span></span>
                                </div>
                                <div class="clear"> </div>
                                <label class="checkbox"><input type="checkbox" name="checkbox" checked><i></i>I agree to the <a href="#">Terms and Conditions</a></label>
                            </div>
                            <input type="submit" value="Register">
                        </form>
                    </div>
                </div>
                <div class="clear"> </div>
            </div>
            <div class="clear"> </div>
        </div>
        <!-- copyright -->
        <p class="agile-copyright">© 2025 All Rights Reserved | Design by <a href="https://w3layouts.com/" target="_blank">W3layouts</a></p>
        <!-- //copyright -->
    </section>
    <!-- //section -->
</body>
<!-- //body ends -->
</html>